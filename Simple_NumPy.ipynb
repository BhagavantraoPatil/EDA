        "import numpy as np\n",
        "\n",
        "bmi = [2.8, 20, 30, 21.5, 15, 21]\n",
        "bmi_np = np.array(bmi)\n",
        "\n",
        "overweight = bmi_np [bmi_np>=21]\n",
        "underweight = bmi_np [bmi_np<21]\n",
        "\n",
        "print (\"Overweight: \",overweight)\n",
        "print (\"Underweight: \",underweight)"
      ]
    }
